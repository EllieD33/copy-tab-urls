{
  "manifest_version": 2,
  "name": "GetTabUrls",
  "version": "1.0",
  "description": "Copies URLs from open tabs to clipboard.",
  "icons": {
    "48": "icons/copy_all_light.png"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "theme"
  ],
  "background": {
    "scripts": ["getTabUrls.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/copy_all_light.png"
    },
    "default_title": "Get Tab URLs"
  }
}

